# n8n Configuration
N8N_HOST=n8n.yourdomain.com
WEBHOOK_URL=https://n8n.yourdomain.com/

# PostgreSQL Database
DB_POSTGRESDB_HOST=your-db-host.example.com
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=neondb
DB_POSTGRESDB_USER=your_db_user
DB_POSTGRESDB_PASSWORD=your_secure_password

# Redis Configuration (usually defaults are fine)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB_QUEUE=0
REDIS_DB_CACHE=1
